{
  "name": "@viaa/avo2-components",
  "version": "1.40.0",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "style": "dist/styles/main.css",
  "types": "dist/index.d.ts",
  "scripts": {
    "analyze": "source-map-explorer 'build/*.js'",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "build-storybook": "build-storybook -o build",
    "build-storybook:with-sourcemaps": "cross-env GENERATE_SOURCEMAP=true build-storybook -o build",
    "format": "npm run lint -- --fix",
    "preicons": "node scripts/icons.js src/icons",
    "icons": "rimraf src/components/Icon/Icons && svgr -d src/components/Icon/Icons src/icons --ext ts",
    "posticons": "named-exports --dir=src/components/Icon/Icons && renamer --find .ts --replace .tsx \"./src/components/Icon/Icons/**\"",
    "lint": "tslint -p .",
    "start": "start-storybook -s ./static -p 3020 --ci",
    "test": "jest --verbose",
    "test:output": "cross-env CI=true jest --json --outputFile=.storybook/test-results.json",
    "licenses": "license-checker --csv --out licenses.csv"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@storybook/addon-options": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@types/autosize": "^3.0.6",
    "@types/classnames": "^2.2.7",
    "@types/jquery": "^3.3.30",
    "@types/marked": "^0.6.5",
    "@types/node": "12.0.2",
    "@types/raf": "^3.4.0",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "autosize": "^4.0.2",
    "babel-plugin-react-docgen": "^4.1.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.0.0-beta.1",
    "flowplayer-files": "^2.2.1",
    "jquery": "^3.5.0",
    "lodash-es": "^4.17.15",
    "marked": "^0.7.0",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-datepicker": "^2.10.0",
    "react-dom": "^16.8.6",
    "react-range": "^1.6.0",
    "react-select": "^3.1.0",
    "trumbowyg": "2.21.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^7.6.0",
    "@sambego/storybook-styles": "^1.0.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-jest": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@svgr/cli": "^4.2.0",
    "@tom-odb/named-exports": "^1.4.6",
    "@types/enzyme": "^3.9.2",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.13",
    "@types/lodash-es": "^4.17.3",
    "@types/react-datepicker": "^2.9.3",
    "@types/react-select": "^3.0.11",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__react": "^4.0.1",
    "autoprefixer": "^9.6.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "glob": "^7.1.4",
    "husky": "^2.3.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^8.1.7",
    "lorem-ipsum": "^2.0.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1",
    "react-scripts": "^3.3.0",
    "react-test-renderer": "^16.8.6",
    "renamer": "^1.1.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.15.6",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-typescript2": "^0.21.2",
    "source-map-explorer": "^2.0.0",
    "storybook-addon-jsx": "^7.1.2",
    "storybook-addon-smart-knobs": "^5.0.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "3.5.3"
  },
  "publishConfig": {
    "@viaa:registry": "http://do-prd-mvn-01.do.viaa.be:8081/repository/npm-viaa/"
  }
}
