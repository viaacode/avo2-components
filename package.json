    "tslib": "2.4.0",
    "typescript": "4.8.3",
    "webpack": "^5.74.0"
  },
  "publishConfig": {
    "@viaa:registry": "http://do-prd-mvn-01.do.viaa.be:8081/repository/npm-viaa/"
  },
  "overrides": {
    "webpack": "^5.74.0"
  },
  "snyk": true
}
