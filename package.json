    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.17",
    "@svgr/cli": "^5.4.0",
    "@tom-odb/named-exports": "^1.4.6",
    "@types/autosize": "^3.0.7",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.10",
    "@types/lodash-es": "^4.17.3",
    "@types/marked": "^4.0.3",
    "@types/node": "14.6.1",
