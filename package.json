{
	"name": "@viaa/avo2-components",
	"version": "1.56.0",
	"main": "dist/bundle.cjs.js",
	"module": "dist/bundle.esm.js",
	"style": "dist/styles/main.css",
	"types": "dist/index.d.ts",
	"homepage": "./",
	"scripts": {
		"analyze": "source-map-explorer 'build/*.js'",
		"prebuild": "rimraf dist",
		"build": "rollup -c",
		"build-storybook": "build-storybook -o build",
		"build-storybook:with-sourcemaps": "cross-env GENERATE_SOURCEMAP=true build-storybook -o build",
		"format": "npm run lint -- --fix",
		"preicons": "node scripts/icons.js src/icons",
		"icons": "rimraf src/components/Icon/Icons && svgr -d src/components/Icon/Icons src/icons --ext ts",
		"posticons": "named-exports --dir=src/components/Icon/Icons && renamer --find .ts --replace .tsx \"./src/components/Icon/Icons/**\"",
		"lint": "tslint -p .",
		"start": "start-storybook -s ./static -p 3020 --ci",
		"test": "jest --verbose",
		"test:output": "cross-env CI=true jest --json --outputFile=.storybook/test-results.json",
		"licenses": "license-checker --csv --out licenses.csv",
		"snyk-protect": "snyk protect"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@types/autosize": "^3.0.7",
		"@types/classnames": "^2.2.10",
		"@types/marked": "^1.1.0",
		"@types/node": "14.6.1",
		"@types/raf": "^3.4.0",
		"@types/react": "16.9.48",
		"@types/react-dom": "16.9.8",
		"autosize": "^4.0.2",
		"babel-plugin-react-docgen": "^4.1.0",
		"braft-editor": "^2.3.9",
		"braft-extensions": "^0.1.0",
		"classnames": "^2.2.6",
		"css-loader": "^4.2.2",
		"date-fns": "^2.16.0",
		"flowplayer-files": "^2.5.10",
		"lodash-es": "^4.17.15",
		"marked": "^1.1.1",
		"moment": "^2.27.0",
		"react": "^16.13.1",
		"react-datepicker": "^3.1.3",
		"react-dom": "^16.13.1",
		"react-perfect-scrollbar": "^1.5.8",
		"react-range": "^1.6.0",
		"react-select": "^3.1.0",
		"sass-loader": "^10.0.1",
		"snyk": "^1.385.2",
