{
  "name": "@viaa/avo2-components",
  "version": "1.98.2",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "homepage": "./",
  "scripts": {
    "prepare": "npm run snyk-protect",
    "force-resolutions": "npx npm-force-resolutions",
    "analyze": "source-map-explorer 'build/*.js'",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "build-and-copy-to-avo-client": "rimraf ../avo2-client/node_modules/@viaa/avo2-components/dist ../avo2-client/node_modules/@viaa/avo2-components/src ../avo2-client/node_modules/.cache && npm run build && cp -r dist src package.json ../avo2-client/node_modules/@viaa/avo2-components",
    "build-and-copy-to-hetarchief-client": "rimraf ../hetarchief-client/node_modules/@viaa/avo2-components/dist ../hetarchief-client/node_modules/@viaa/avo2-components/src ../hetarchief-client/node_modules/.cache && npm run build && cp -r dist src package.json ../hetarchief-client/node_modules/@viaa/avo2-components",
    "build-and-copy-to-admin-core": "rimraf ../react-admin-core-module/demo/node_modules/@viaa/avo2-components/dist ../react-admin-core-module/demo/node_modules/@viaa/avo2-components/src ../react-admin-core-module/demo/node_modules/.cache && npm run build && cp -r dist src package.json ../react-admin-core-module/demo/node_modules/@viaa/avo2-components",
    "build-storybook": "build-storybook -o build",
    "build-storybook:with-sourcemaps": "cross-env GENERATE_SOURCEMAP=true build-storybook -o build",
    "preicons": "node scripts/icons.js src/icons",
    "icons": "rimraf src/components/Icon/Icons && svgr -d src/components/Icon/Icons src/icons --ext ts",
    "posticons": "named-exports --dir=src/components/Icon/Icons && renamer --find .ts --replace .tsx \"./src/components/Icon/Icons/**\"",
    "lint": "eslint \"./{src,scripts}/**/*.{ts,tsx}\" --cache",
    "lint:fix": "eslint \"./{src,scripts}/**/*.{ts,tsx}\" --fix",
    "format": "prettier --list-different \"{src,scripts}/**/*.{ts,tsx}\"",
    "format:fix": "prettier --list-different --write \"{src,scripts}/**/*.{ts,tsx}\"",
    "start": "start-storybook -s ./static -p 3020 --ci",
    "test": "jest --verbose",
    "test:output": "cross-env CI=true jest --json --outputFile=.storybook/test-results.json",
    "licenses": "license-checker --csv --out licenses.csv",
    "snyk-protect": "snyk-protect"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react-scrollbars-custom": "^4.1.1"
  },
  "dependencies": {
    "@flowplayer/player": "3.2.8",
    "@popperjs/core": "^2.5.4",
    "autosize": "^4.0.2",
    "braft-editor": "^2.3.9",
    "braft-extensions": "^0.1.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.0",
    "lodash-es": "^4.17.21",
    "marked": "^4.0.15",
    "moment": "^2.29.2",
    "raf": "^3.4.1",
    "react": "^17.0.2",
    "react-datepicker": "^3.1.3",
    "react-dom": "^17.0.2",
    "react-perfect-scrollbar": "^1.5.8",
    "react-popper": "^2.2.4",
    "react-range": "^1.6.0",
    "react-select": "^3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@snyk/protect": "^1.1038.0",
    "@storybook/addon-actions": "^6.1.9",
    "@storybook/addon-essentials": "^6.4.17",
    "@storybook/addons": "^6.1.9",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.4.17",
    "@svgr/cli": "^5.4.0",
    "@tom-odb/named-exports": "^1.4.6",
    "@types/autosize": "^3.0.7",
    "@types/enzyme": "^3.10.5",
    "@types/lodash-es": "^4.17.3",
    "@types/marked": "^4.0.3",
    "@types/raf": "^3.4.0",
    "@types/react": "^17.0.2",
    "@types/react-datepicker": "^3.1.1",
    "@types/react-dom": "^17.0.2",
    "@types/react-select": "^3.1.2",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "autoprefixer": "^9.8.6",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "eslint": "8.22.0",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "license-checker": "^25.0.1",
    "lint-staged": "^10.2.13",
    "lorem-ipsum": "^2.0.3",
    "prettier": "^2.1.1",
    "react-scripts": "^3.4.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-postcss": "^3.1.6",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "sass": "^1.55.0",
    "source-map-explorer": "^2.5.0",
    "storybook-addon-jsx": "^7.3.4",
    "ts-jest": "^26.3.0",
    "typescript": "^4.1"
  },
  "publishConfig": {
    "@viaa:registry": "http://do-prd-mvn-01.do.viaa.be:8081/repository/npm-viaa/"
  },
  "resolutions": {
    "eslint-loader": "4.0.2",
    "isomorphic-fetch": "3.0.0"
  },
  "snyk": true
}
