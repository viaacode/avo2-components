{
  "name": "@viaa/avo2-components",
  "version": "3.6.14",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "homepage": "./",
  "scripts": {
    "analyze": "source-map-explorer 'build/*.js'",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "copy-to-avo-client": "rimraf ../avo2-client/node_modules/@viaa/avo2-components/dist ../avo2-client/node_modules/@viaa/avo2-components/src ../avo2-client/node_modules/.cache && cp -r dist src package.json ../avo2-client/node_modules/@viaa/avo2-components",
    "copy-to-hetarchief-client": "rimraf ../hetarchief-client/node_modules/@viaa/avo2-components/dist ../hetarchief-client/node_modules/@viaa/avo2-components/src ../hetarchief-client/node_modules/.cache && cp -r dist src package.json ../hetarchief-client/node_modules/@viaa/avo2-components",
    "copy-to-admin-core-ui": "rimraf ../react-admin-core-module/ui/node_modules/@viaa/avo2-components/dist ../react-admin-core-module/ui/node_modules/@viaa/avo2-components/src ../react-admin-core-module/ui/node_modules/.cache && cp -r dist src package.json ../react-admin-core-module/ui/node_modules/@viaa/avo2-components",
    "copy": "npm run build && npm run copy-to-avo-client && npm run copy-to-hetarchief-client && npm run copy-to-admin-core-ui",
    "build-storybook": "build-storybook -o build",
    "build-storybook:with-sourcemaps": "cross-env GENERATE_SOURCEMAP=true build-storybook -o build",
    "preicons": "node scripts/icons.js src/icons",
    "icons": "rimraf src/components/Icon/Icons && svgr -d src/components/Icon/Icons src/icons --ext tsx && rm -rf src/components/Icon/Icons/index.ts",
    "posticons": "named-exports --dir=src/components/Icon/Icons && renamer --find \"\\.ts$\" --replace .tsx \"./src/components/Icon/Icons/**\" && rm -rf src/components/Icon/Icons/index.ts && renamer --find index.tsx --replace index.ts \"./src/components/Icon/Icons/**\" && echo \"export * from './arrows'; export * from './custom';export * from './multicolor';export * from './social';export * from './wysiwyg';\"",
    "lint": "eslint \"./{src,scripts}/**/*.{ts,tsx}\" --cache",
    "lint:fix": "eslint \"./{src,scripts}/**/*.{ts,tsx}\" --fix",
    "format": "prettier --list-different \"{src,scripts}/**/*.{ts,tsx}\"",
    "format:fix": "prettier --list-different --write \"{src,scripts}/**/*.{ts,tsx}\"",
    "start": "start-storybook -p 3020 --ci",
    "test": "jest --verbose",
    "test:output": "cross-env CI=true jest --json --outputFile=.storybook/test-results.json",
    "licenses": "license-checker --csv --out licenses.csv",
    "snyk-protect": "snyk protect",
    "bump": "bump-package-versions --part=patch --strategy=highest ./package.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "autosize": "^4.0.2",
    "caniuse-lite": "^1.0.30001482",
    "marked": "^4.0.15",
    "moment": "^2.29.4",
    "npm-force-resolutions": "^0.0.10",
    "raf": "^3.4.1",
    "react-datepicker": "^4.8.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-popper": "^2.3.0",
    "react-range": "^1.6.0",
    "react-scrollbars-custom": "^4.1.1",
    "react-select": "5.7.3",
    "@popperjs/core": "^2.11.6",
    "@viaa/avo2-types": "^2.48.29",
    "clsx": "^1.2.1",
    "date-fns": "^2.16.0",
    "lodash-es": "^4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@popperjs/core": "^2.11.6",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@snyk/protect": "^1.1038.0",
    "@storybook/addon-actions": "^6.5.11",
    "@storybook/addon-essentials": "^6.5.11",
    "@storybook/addons": "^6.5.11",
    "@storybook/builder-webpack5": "^6.5.11",
    "@storybook/manager-webpack5": "^6.5.11",
