    "date-fns": "^2.16.0",
    "lodash-es": "^4.17.21",
    "marked": "^4.0.15",
    "moment": "^2.29.2",
    "react-datepicker": "^3.1.3",
    "react-dom": "^16.13.1",
    "react-perfect-scrollbar": "^1.5.8",
    "react-popper": "^2.2.4",
    "react-range": "^1.6.0",
    "react-select": "^3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-multi-entry": "^4.0.0",
