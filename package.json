{
  "name": "@viaa/avo2-components",
  "version": "6.0.12",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "homepage": "./",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/*.js'",
    "build": "vite build",
    "copy-to-avo-client": "rimraf ../avo2-client/node_modules/@viaa/avo2-components/dist ../avo2-client/node_modules/@viaa/avo2-components/src ../avo2-client/node_modules/.cache ../avo2-client/node_modules/.vite && cp -r dist src package.json package-lock.json ../avo2-client/node_modules/@viaa/avo2-components",
    "copy-to-hetarchief-client": "rimraf ../hetarchief-client/node_modules/@viaa/avo2-components/dist ../hetarchief-client/node_modules/@viaa/avo2-components/src ../hetarchief-client/node_modules/.cache ../hetarchief-client/node_modules/.vite && cp -r dist src package.json package-lock.json ../hetarchief-client/node_modules/@viaa/avo2-components",
    "copy-to-admin-core-ui": "rimraf ../react-admin-core-module/ui/node_modules/@viaa/avo2-components/dist ../react-admin-core-module/ui/node_modules/@viaa/avo2-components/src ../react-admin-core-module/ui/node_modules/.cache ../react-admin-core-module/ui/node_modules/.vite && cp -r dist src package.json package-lock.json ../react-admin-core-module/ui/node_modules/@viaa/avo2-components",
    "copy": "vite build && npm run copy-to-avo-client && npm run copy-to-hetarchief-client && npm run copy-to-admin-core-ui",
    "build-storybook": "build-storybook -o build",
    "build-storybook:with-sourcemaps": "cross-env GENERATE_SOURCEMAP=true build-storybook -o build",
    "preicons": "node scripts/icons.js src/icons",
    "icons": "rimraf src/components/Icon/Icons && svgr -d src/components/Icon/Icons src/icons --ext tsx && rm -rf src/components/Icon/Icons/index.ts",
    "posticons": "named-exports --dir=src/components/Icon/Icons && renamer --find \"\\.ts$\" --replace .tsx \"./src/components/Icon/Icons/**\" && rm -rf src/components/Icon/Icons/index.ts && renamer --find index.tsx --replace index.ts \"./src/components/Icon/Icons/**\" && echo \"export * from './arrows'; export * from './custom';export * from './multicolor';export * from './social';export * from './wysiwyg';\"",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "biome lint --write",
    "lint:scss": "stylelint 'src/**/*.{css,scss}' --allow-empty-input --fix",
    "format": "biome format --write",
    "dev": "storybook dev -p 3020",
    "test": "jest --verbose",
    "analyse": "source-map-explorer 'dist/index.mjs' --no-border-checks",
    "test:output": "cross-env CI=true jest --json --outputFile=.storybook/test-results.json",
    "licenses": "license-checker --csv --out licenses.csv",
    "snyk-protect": "snyk protect",
    "bump": "bump-package-versions --part=patch --strategy=highest ./package.json",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "@popperjs/core": "2.11.6",
    "@viaa/avo2-types": "4.0.2",
    "autosize": "6.0.1",
    "clsx": "2.0.0",
    "date-fns": "3.6.0",
    "marked": "4.0.15",
    "raf": "3.4.1",
    "react": "18.3.1",
    "react-datepicker": "8.9.0",
    "react-dom": "18.3.1",
    "react-perfect-scrollbar": "1.5.8",
    "react-popper": "2.3.0",
    "react-range": "1.8.14",
    "react-select": "5.10.2",
    "to-camel-case": "1.0.0",
    "to-slug-case": "1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@snyk/protect": "1.1221.0",
    "@storybook/addon-docs": "10.0.6",
    "@storybook/react": "10.0.6",
    "@storybook/react-vite": "^10.0.6",
    "@svgr/cli": "5.5.0",
    "@tom-odb/named-exports": "1.4.6",
    "@types/autosize": "3.0.7",
    "@types/enzyme": "3.10.13",
    "@types/jest": "26.0.24",
    "@types/marked": "4.3.1",
    "@types/node": "22.15.29",
    "@types/raf": "3.4.0",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/storybook__addon-actions": "5.2.1",
    "@types/storybook__react": "5.2.1",
    "@vitejs/plugin-react": "5.1.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.8.0",
    "autoprefixer": "9.8.8",
    "babel-plugin-react-docgen": "4.2.1",
    "bump-package-versions": "^1.0.7",
    "core-js": "3.32.2",
    "cross-env": "7.0.3",
    "css-loader": "4.3.0",
    "enzyme": "3.11.0",
    "es-toolkit": "1.41.0",
